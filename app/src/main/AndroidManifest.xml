<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="mx.emp.primerproyecto">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.PrimerProyecto"
        tools:targetApi="31">
        <activity
            android:name=".fragments.FragmentManagerActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".fragments.FragmentActivity"
            android:exported="true"/>
        <activity
            android:name=".layouts.TapsActivity"
            android:exported="false" />
        <activity
            android:name=".layouts.RecyclerViewActivity"
            android:exported="true" />
        <activity
            android:name=".layouts.RelativeActivity"
            android:exported="false" />
        <activity
            android:name=".ActividadUnoSecondActivity"
            android:exported="false" />
        <activity
            android:name=".ActividadUnoFirstActivity"
            android:exported="false" />
        <activity
            android:name=".ExplicitDetailActivity"
            android:exported="false" />
        <activity
            android:name=".ExplicitActivity"
            android:exported="false" />
        <activity
            android:name=".ImplicitActivity"
            android:exported="false" />
        <activity
            android:name=".MainActivity"
            android:exported="false" />
    </application>

</manifest>